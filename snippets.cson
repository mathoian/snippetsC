'.source.c,.source.c\\+\\+,.source.cpp':
  'matriz':
   'prefix': 'mat'
   'body': 'for (size_t x = 0; x < ${1:code}; x++){\n
           for (size_t y = 0; y < ${2:code}; y++){\n
              ${3:/*code*/}\n
            }\n
      }\n'
